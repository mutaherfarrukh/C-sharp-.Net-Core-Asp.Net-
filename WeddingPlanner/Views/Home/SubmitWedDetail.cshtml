@model WeddingUsers

<div id="maincontainer">
<h1 class="text-center">Plan your Wedding</h1>
<h4 class="text-right mt-3"><a href="/dashboard" class="btn btn-lg btn-primary" >Dashboard</a><a href="/logout" class="btn btn-lg btn-warning">Log Out</a></h4>
<form asp-controller="Home" asp-action="SubmitWedList">
        <div class="form-group">
        <label asp-for="WedCoupleName1"></label>
        <input asp-for="WedCoupleName1" class="form-control">
        <span asp-validation-for="WedCoupleName1" class="alert-danger"></span>
        </div>

        <div class="form-group">
        <label asp-for="WedCoupleName2"></label>
        <input asp-for="WedCoupleName2" class="form-control">
        <span asp-validation-for="WedCoupleName2" class="alert-danger"></span>
        </div>

        <div class="form-group">
        <label asp-for="Date"></label>
        <input asp-for="Date" class="form-control">
        <span asp-validation-for="Date" class="alert-danger"></span>
        </div>

        <div class="form-group">
        <label asp-for="WeddingAddress"></label>
        <input asp-for="WeddingAddress" class="form-control">
        <span asp-validation-for="WeddingAddress" class="alert-danger"></span>
        </div>

        <input type="submit" class="btn btn-lg btn-primary" value="Create">
</form>
</div>